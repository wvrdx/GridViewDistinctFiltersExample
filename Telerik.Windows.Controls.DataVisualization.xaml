                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                                <TextBlock
                                    Margin="{TemplateBinding Padding}"
                                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                    Text="{Binding Label}" />
                            </Border>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style BasedOn="{StaticResource TimelineStripLineControlStyle}" TargetType="timelines:TimelineStripLineControl" />
    <Style BasedOn="{StaticResource TimelinePeriodControlStyle}" TargetType="timelines:TimelinePeriodControl" />
    <Style BasedOn="{StaticResource TimelineGroupPeriodControlStyle}" TargetType="timelines:TimelineGroupPeriodControl" />
    <Style x:Key="TimelineItemGroupControlToggleButtonStyle" TargetType="telerik1:RadToggleButton">
        <Setter Property="mat:MaterialAssist.MouseOverBrush" Value="{telerik1:FluentResource ResourceKey=MouseOverBrush}" />
        <Setter Property="mat:MaterialAssist.PressedBrush" Value="{telerik1:FluentResource ResourceKey=PressedBrush}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="CornerRadius" Value="{telerik1:FluentResource ResourceKey=CornerRadius}" />
        <Setter Property="Padding" Value="3,0,0,0" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik1:RadToggleButton">
                    <Grid Background="#01FFFFFF">
                        <StackPanel Orientation="Horizontal">
                            <Grid>
                                <Border
                                    x:Name="BorderVisual"
                                    Background="{TemplateBinding Background}"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    BorderThickness="{TemplateBinding BorderThickness}"
                                    CornerRadius="{TemplateBinding CornerRadius}"
                                    SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                                <mat:FluentControl
                                    BorderThickness="{TemplateBinding BorderThickness}"
                                    CornerRadius="{TemplateBinding CornerRadius}"
                                    EffectMode="Glow"
                                    IsSmartClipped="True"
                                    SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                                    <Grid>
                                        <telerik:RadGlyph
                                            x:Name="expandButton"
                                            FontSize="16"
                                            Foreground="{telerik1:FluentResource ResourceKey=IconBrush}"
                                            Glyph="{StaticResource GlyphPlus}" />
                                        <telerik:RadGlyph
                                            x:Name="collapseButton"
                                            FontSize="16"
                                            Foreground="{telerik1:FluentResource ResourceKey=IconBrush}"
                                            Glyph="{StaticResource GlyphMinus}"
                                            Visibility="Collapsed" />
                                    </Grid>
                                </mat:FluentControl>
                            </Grid>
                            <TextBlock
                                x:Name="groupContent"
                                Margin="{TemplateBinding Padding}"
                                Text="{TemplateBinding Content}" />
                        </StackPanel>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Background" Value="{Binding RelativeSource={RelativeSource Self}, Path=(mat:MaterialAssist.MouseOverBrush), Mode=OneWay}" />
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Background" Value="{Binding RelativeSource={RelativeSource Self}, Path=(mat:MaterialAssist.PressedBrush), Mode=OneWay}" />
                        </Trigger>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter TargetName="collapseButton" Property="Visibility" Value="Visible" />
                            <Setter TargetName="expandButton" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="expandButton" Property="Opacity" Value="{telerik1:FluentResource ResourceKey=DisabledOpacity}" />
                            <Setter TargetName="collapseButton" Property="Opacity" Value="{telerik1:FluentResource ResourceKey=DisabledOpacity}" />
                            <Setter TargetName="groupContent" Property="Opacity" Value="{telerik1:FluentResource ResourceKey=DisabledOpacity}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="TimelineItemGroupControlStyle" TargetType="timelines:TimelineItemGroupControl">
        <Setter Property="FontFamily" Value="{telerik1:FluentResource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:FluentResource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik1:FluentResource ResourceKey=MarkerBrush}" />
        <Setter Property="Padding" Value="10,0,5,0" />
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <timelines:VirtualizingTimelinePanel Background="#01FFFFFF" Slider="{Binding ElementName=PART_Slider}" />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Header" Value="{Binding GroupKey}" />
        <Setter Property="ItemsSource" Value="{Binding DataItems}" />
        <Setter Property="ItemTemplateSelector" Value="{Binding Path=ItemTemplateSelector, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type datavizControls:RadTimeline}}}" />
        <Setter Property="DataGroups" Value="{Binding DataGroups}" />
        <Setter Property="ExpandMode" Value="{Binding GroupExpandMode, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=datavizControls:RadTimeline}}" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="animation:AnimationManager.AnimationSelector">
            <Setter.Value>
                <animation:AnimationSelector>
                    <animation:ExpanderExpandCollapseAnimation
                        AnimationName="Expand"
                        Direction="In"
                        TargetElementName="Content" />
                    <animation:ExpanderExpandCollapseAnimation
                        AnimationName="Collapse"
                        Direction="Out"
                        TargetElementName="Content" />
                    <timelines:TimelineResizeAnimation AnimationName="Resize" TargetElementName="Content" />
                </animation:AnimationSelector>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="timelines:TimelineItemGroupControl">
                    <Border
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>
                            <Grid
                                x:Name="Header"
                                Grid.Row="0"
                                Visibility="{Binding Path=HasHeader, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource booleanToVisibilityConverter}}">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <telerik1:RadToggleButton
                                    x:Name="HeaderButton"
                                    Grid.Column="0"
                                    Margin="{TemplateBinding Padding}"
                                    Content="{TemplateBinding Header}"
                                    ContentTemplate="{TemplateBinding HeaderTemplate}"
                                    Foreground="{TemplateBinding Foreground}"
                                    IsChecked="{Binding IsExpanded, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}"
                                    IsEnabled="{Binding ExpandMode, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource groupExpandModeToBooleanConverter}}"
                                    Style="{StaticResource TimelineItemGroupControlToggleButtonStyle}" />
                                <Path
                                    Grid.Column="1"
                                    Height="2"
                                    Margin="0,1,0,0"
                                    HorizontalAlignment="Stretch"
                                    VerticalAlignment="Center"
                                    Data="{StaticResource LineIcon}"
                                    Stretch="Fill"
                                    Stroke="{telerik1:FluentResource ResourceKey=BasicBrush}" />
                            </Grid>
                            <ItemsPresenter x:Name="Content" Grid.Row="1" />
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style BasedOn="{StaticResource TimelineItemGroupControlStyle}" TargetType="timelines:TimelineItemGroupControl" />
    <sys:Double x:Key="TimelineItemDimensions">10</sys:Double>
    <Style x:Key="TimelinePeriodHeaderStyle" TargetType="Border">
        <Setter Property="Height" Value="28" />
        <Setter Property="VerticalAlignment" Value="Top" />
        <Setter Property="Margin" Value="0,2,0,0" />
    </Style>
    <Style x:Key="TimelineGroupPeriodHeaderStyle" TargetType="Border">
        <Setter Property="Height" Value="28" />
        <Setter Property="VerticalAlignment" Value="Top" />
    </Style>
    <Style x:Key="TimelineItemControlStyle" TargetType="timelines:TimelineItemControl">
        <Setter Property="Height" Value="{StaticResource TimelineItemDimensions}" />
        <Setter Property="Margin" Value="0,5" />
        <Setter Property="Background" Value="{telerik1:FluentResource ResourceKey=AccentBrush}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="timelines:TimelineItemControl">
                    <Grid Background="#01FFFFFF">
                        <Rectangle x:Name="timelineItem" Fill="{TemplateBinding Background}" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsSelected" Value="True">
                            <Setter Property="Background" Value="{telerik1:FluentResource ResourceKey=AccentFocusedBrush}" />
                        </Trigger>
                        <Trigger Property="IsHovered" Value="True">
                            <Setter Property="Background" Value="{telerik1:FluentResource ResourceKey=AccentMouseOverBrush}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="TimelineInstantItemControlStyle" TargetType="timelines:TimelineInstantItemControl">
        <Setter Property="Width" Value="{StaticResource TimelineItemDimensions}" />
        <Setter Property="Height" Value="{StaticResource TimelineItemDimensions}" />
        <Setter Property="Margin" Value="0,5" />
        <Setter Property="Background" Value="{telerik1:FluentResource ResourceKey=IconBrush}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="timelines:TimelineInstantItemControl">
                    <Grid Background="#01FFFFFF" RenderTransformOrigin=".5 .5">
                        <Rectangle
                            x:Name="timelineItem"
                            Width="16"
                            Height="16"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            Fill="{TemplateBinding Background}" />
                        <Grid.RenderTransform>
                            <RotateTransform Angle="45" />
                        </Grid.RenderTransform>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsSelected" Value="True">
                            <Setter Property="Background" Value="{telerik1:FluentResource ResourceKey=AccentFocusedBrush}" />
                        </Trigger>
                        <Trigger Property="IsHovered" Value="True">
                            <Setter Property="Background" Value="{telerik1:FluentResource ResourceKey=AccentMouseOverBrush}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <DataTemplate x:Key="TimelineItemTemplate">
        <timelines:TimelineItemControl />
    </DataTemplate>
    <DataTemplate x:Key="TimelineInstantItemTemplate">
        <timelines:TimelineInstantItemControl />
    </DataTemplate>
    <Style x:Key="TimelineAnnotationStyle" TargetType="timelines:TimelineAnnotation">
        <Setter Property="IsHitTestVisible" Value="False" />
        <Setter Property="Margin" Value="0,28,0,0" />
        <Setter Property="Canvas.ZIndex" Value="300" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="timelines:TimelineAnnotation">
                    <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="SelectionRegionStyle" TargetType="timelines:SelectionRegion">
        <Setter Property="Visibility" Value="Collapsed" />
        <Setter Property="BorderBrush" Value="{telerik1:FluentResource ResourceKey=IconBrush}" />
        <Setter Property="BorderThickness" Value="2" />
        <Setter Property="Background" Value="{telerik1:FluentResource ResourceKey=AccentMouseOverBrush}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="timelines:SelectionRegion">
                    <Grid>
                        <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                            <Rectangle Fill="{TemplateBinding Background}" Opacity="0.20" />
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="TimelineItemContainerStyle" TargetType="timelines:TimelineItemContainer">
        <Setter Property="Canvas.ZIndex" Value="200" />
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <timelines:TimelineItemGroupsPanel Padding="0,58,0,1" VerticalScrollBar="{Binding ElementName=Part_VerticalSlider}" />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadTimelineStyle" TargetType="datavizControls:RadTimeline">
        <Setter Property="FontFamily" Value="{telerik1:FluentResource ResourceKey=FontFamily}" />
        <Setter Property="Foreground" Value="{telerik1:FluentResource ResourceKey=MarkerBrush}" />
        <Setter Property="Background" Value="{telerik1:FluentResource ResourceKey=PrimaryBackgroundBrush}" />
        <Setter Property="Padding" Value="0" />
        <Setter Property="BorderBrush" Value="{telerik1:FluentResource ResourceKey=BasicSolidBrush}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="SliderStyle" Value="{StaticResource Timeline_SliderStyle}" />
        <Setter Property="TimelineItemTemplate" Value="{StaticResource TimelineItemTemplate}" />
        <Setter Property="TimelineInstantItemTemplate" Value="{StaticResource TimelineInstantItemTemplate}" />
        <Setter Property="PeriodHeaderStyle" Value="{StaticResource TimelinePeriodHeaderStyle}" />
        <Setter Property="GroupPeriodHeaderStyle" Value="{StaticResource TimelineGroupPeriodHeaderStyle}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="datavizControls:RadTimeline">
                    <Border
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}"
                        SnapsToDevicePixels="True">
                        <Grid Margin="{TemplateBinding Padding}">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <timelines:TimelineAnnotationsPanel
                                x:Name="PART_TimelineAnnotationsPanel"
                                Grid.Row="0"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                Slider="{Binding ElementName=PART_Slider}">
                                <Grid Canvas.ZIndex="100">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <Border
                                        Grid.Row="1"
                                        Grid.RowSpan="2"
                                        Background="{TemplateBinding Background}" />
                                    <timelines:TimelineStripLineContainer
                                        x:Name="PART_TimelineStripLineContainer"
                                        Grid.Row="2"
                                        timeBars:ActualSizeProxy.IsEnabled="True"
                                        ItemsSource="{TemplateBinding IntervalItems}">
                                        <timelines:TimelineStripLineContainer.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <timeBars:VirtualizingTimeBarPanel Slider="{Binding ElementName=PART_Slider}" />
                                            </ItemsPanelTemplate>
                                        </timelines:TimelineStripLineContainer.ItemsPanel>
                                        <timelines:TimelineStripLineContainer.Template>
                                            <ControlTemplate>
                                                <ItemsPresenter />
                                            </ControlTemplate>
                                        </timelines:TimelineStripLineContainer.Template>
                                    </timelines:TimelineStripLineContainer>
                                    <timeBars:TimeSpecialSlotContainer
                                        x:Name="PART_TimeSpecialSlotContainer"
                                        Grid.Row="2"
                                        ItemsSource="{TemplateBinding SpecialSlots}">
                                        <timeBars:TimeSpecialSlotContainer.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <timeBars:TimeBarPanel Slider="{Binding ElementName=PART_Slider}" />
                                            </ItemsPanelTemplate>
                                        </timeBars:TimeSpecialSlotContainer.ItemsPanel>
                                        <timeBars:TimeSpecialSlotContainer.Template>
                                            <ControlTemplate>
                                                <ItemsPresenter />
                                            </ControlTemplate>
                                        </timeBars:TimeSpecialSlotContainer.Template>
                                    </timeBars:TimeSpecialSlotContainer>
                                    <timelines:TimelineGroupPeriodContainer
                                        x:Name="PART_TimelineGroupPeriodContainer"
                                        Grid.Row="0"
                                        ItemHeaderStyle="{TemplateBinding GroupPeriodHeaderStyle}"
                                        ItemsSource="{TemplateBinding IntervalGroups}">
                                        <timelines:TimelineGroupPeriodContainer.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <timeBars:LabelMeasurementPanel Slider="{Binding ElementName=PART_Slider}" />
                                            </ItemsPanelTemplate>
                                        </timelines:TimelineGroupPeriodContainer.ItemsPanel>
                                        <timelines:TimelineGroupPeriodContainer.Template>
                                            <ControlTemplate>
                                                <ItemsPresenter />
                                            </ControlTemplate>
                                        </timelines:TimelineGroupPeriodContainer.Template>
                                    </timelines:TimelineGroupPeriodContainer>
                                    <timelines:TimelinePeriodContainer
                                        x:Name="PART_TimelinePeriodContainer"
                                        Grid.Row="1"
                                        Grid.RowSpan="2"
                                        ItemHeaderStyle="{TemplateBinding PeriodHeaderStyle}"
                                        ItemsSource="{TemplateBinding IntervalItems}">
                                        <timelines:TimelinePeriodContainer.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <timeBars:LabelMeasurementPanel Slider="{Binding ElementName=PART_Slider}" />
                                            </ItemsPanelTemplate>
                                        </timelines:TimelinePeriodContainer.ItemsPanel>
                                        <timelines:TimelinePeriodContainer.Template>
                                            <ControlTemplate>
                                                <ItemsPresenter />
                                            </ControlTemplate>
                                        </timelines:TimelinePeriodContainer.Template>
                                    </timelines:TimelinePeriodContainer>
                                    <Border
                                        Grid.Row="1"
                                        Grid.RowSpan="2"
                                        Background="{x:Null}"
                                        BorderBrush="{TemplateBinding BorderBrush}"
                                        BorderThickness="1,2,1,1" />
                                </Grid>
                                <timelines:TimelineItemContainer
                                    x:Name="PART_TimelineItemContainer"
                                    GroupExpandMode="{TemplateBinding GroupExpandMode}"
                                    ItemsSource="{TemplateBinding GroupedDataItems}"
                                    SelectionRegionControl="{Binding ElementName=PART_SelectionRegion}">
                                    <timelines:TimelineItemContainer.Template>
                                        <ControlTemplate>
                                            <Grid Background="#01FFFFFF">
                                                <ItemsPresenter />
                                            </Grid>
                                        </ControlTemplate>
                                    </timelines:TimelineItemContainer.Template>
                                </timelines:TimelineItemContainer>
                            </timelines:TimelineAnnotationsPanel>
                            <timelines:SelectionRegionPanel
                                x:Name="PART_SelectionRegionPanel"
                                Grid.Row="0"
                                Grid.Column="0"
                                Slider="{Binding ElementName=PART_Slider}">
                                <timelines:SelectionRegion x:Name="PART_SelectionRegion" />
                            </timelines:SelectionRegionPanel>
                            <timelines:TimelineScrollBar
                                x:Name="Part_VerticalSlider"
                                Grid.Row="0"
                                Grid.Column="1"
                                Height="{Binding ElementName=PART_TimelineStripLineContainer, Path=(timeBars:ActualSizeProxy.ActualHeight)}"
                                MinWidth="16"
                                VerticalAlignment="Bottom"
                                IsDirectionReversed="True"
                                Maximum="1"
                                Minimum="0"
                                Orientation="Vertical"
                                ScrollBarVisibility="{TemplateBinding VerticalScrollBarVisibility}"
                                SelectionEnd="1"
                                SelectionStart="0"
                                Style="{TemplateBinding SliderStyle}"
                                ThumbVisibility="Collapsed"
                                Visibility="Collapsed" />
                            <telerik1:RadSlider
                                x:Name="PART_Slider"
                                Grid.Row="1"
                                Grid.Column="0"
                                Width="Auto"
                                IsCoercionSuppressed="True"
                                LargeChange="{Binding Path=LargeChange, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource timespanConverter}}"
                                Maximum="{Binding Path=PeriodEnd, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource dateConverter}}"
                                MaximumRangeSpan="{Binding Path=MaxZoomRange, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource timeSpanToMaximumRangeSpanConverter}}"
                                Minimum="{Binding Path=PeriodStart, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource dateConverter}}"
                                MinimumRangeSpan="{Binding Path=ActualMinZoomRange, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource timespanConverter}}"
                                Selection="{Binding Path=ActualVisiblePeriodRange, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, Converter={StaticResource selectionRangeConverter}}"
                                SmallChange="{Binding Path=SmallChange, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource timespanConverter}}"
                                Style="{TemplateBinding SliderStyle}"
                                ThumbVisibility="{Binding Path=ScrollMode, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource scrollModeToThumbVisibilityConverter}}"
                                Visibility="{Binding Path=ScrollMode, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource scrollModeToVisibilityConverter}}" />
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style BasedOn="{StaticResource TimelineItemControlStyle}" TargetType="timelines:TimelineItemControl" />
    <Style BasedOn="{StaticResource TimelineInstantItemControlStyle}" TargetType="timelines:TimelineInstantItemControl" />
    <Style BasedOn="{StaticResource TimelineAnnotationStyle}" TargetType="timelines:TimelineAnnotation" />
    <Style BasedOn="{StaticResource SelectionRegionStyle}" TargetType="timelines:SelectionRegion" />
    <Style BasedOn="{StaticResource TimelineItemContainerStyle}" TargetType="timelines:TimelineItemContainer" />
    <Style BasedOn="{StaticResource RadTimelineStyle}" TargetType="datavizControls:RadTimeline" />
</ResourceDictionary>
