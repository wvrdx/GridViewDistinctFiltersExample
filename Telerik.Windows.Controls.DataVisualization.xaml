                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                FontSize="16"
                                Glyph="{StaticResource GlyphZoom}" />
                        </telerik:RadDropDownButton>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
