                                <ContentControl x:Name="Track" Style="{TemplateBinding TrackStyle}" />
                                <timeBars:TimeBarNonScrollingPanel
                                    Slider="{Binding ElementName=PART_Slider}"
                                    SliderMaximum="{Binding ElementName=PART_Slider, Path=Maximum, Mode=TwoWay}"
                                    SliderMinimum="{Binding ElementName=PART_Slider, Path=Minimum, Mode=TwoWay}">
                                    <timeBars:SelectionIndicator EndDate="{Binding ElementName=PART_SelectionThumb, Path=EndDate}" StartDate="{Binding ElementName=PART_SelectionThumb, Path=StartDate}" />
