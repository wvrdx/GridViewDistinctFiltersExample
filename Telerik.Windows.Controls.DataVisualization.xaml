            <Setter.Value>
                <DataTemplate>
                    <telerik:RadRadioButton
                        MinWidth="110"
                        Margin="2"
                        Command="{Binding Path=Command}"
                        CommandParameter="{Binding Path=CommandParameter}"
                        Content="{Binding Path=Command.Text}"
                        FontSize="{telerik1:FluentResource ResourceKey=FontSize}"
                        GroupName="ZoomLevelButtons" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
