    <Style x:Key="TimelineGroupPeriodControlStyle" TargetType="timelines:TimelineGroupPeriodControl">
        <Setter Property="Foreground" Value="{telerik1:FluentResource ResourceKey=AccentBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:FluentResource ResourceKey=BasicSolidBrush}" />
        <Setter Property="FontSize" Value="{telerik1:FluentResource ResourceKey=FontSize}" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="BorderThickness" Value="0,0,1,1" />
        <Setter Property="Padding" Value="5,0,5,3" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
