                            DropDownPlacement="Top"
                            HoverDelay="0:0:0.01"
                            KeepOpen="True"
                            Style="{StaticResource MapZoomBarButtonStyle}">
                            <telerik:RadDropDownButton.DropDownContent>
                                <Grid
                                    x:Name="PART_DropDownPopupWrapper"
                                    Margin="3,3,3,10"
                                    Background="#01ffffff">
                                    <Grid Width="42" HorizontalAlignment="Left">
                                        <mat:Shadow
                                            x:Name="PopupShadow"
                                            Background="{TemplateBinding Background}"
                                            CornerRadius="{TemplateBinding mat:MaterialAssist.CornerRadius}"
                                            ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}"
                                            SnapsToDevicePixels="True" />
                                        <Border Background="{TemplateBinding Background}" CornerRadius="{TemplateBinding mat:MaterialAssist.CornerRadius}">
                                            <telerik:RadSlider x:Name="PART_ZoomSlider" Style="{StaticResource MapSliderStyle}" />
                                        </Border>
