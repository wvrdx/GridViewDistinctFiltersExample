    </Style>
    <Style x:Key="Barcode93ExtendedStyle" TargetType="datavizControls:RadBarcode93Extended">
        <Setter Property="Foreground" Value="{telerik1:FluentResource ResourceKey=MarkerBrush}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="datavizControls:RadBarcode93Extended">
                    <Grid x:Name="PART_RadBarcode93Extended" Background="{TemplateBinding Background}">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
