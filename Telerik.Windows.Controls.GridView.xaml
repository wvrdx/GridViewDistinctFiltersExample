    <Style x:Key="TreeListViewDragCueStyle" TargetType="controls:TreeListViewDragCue">
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1" />
        <Setter Property="IsHitTestVisible" Value="False" />
        <Setter Property="Padding" Value="5" />
        <Setter Property="Foreground" Value="{telerik1:FluentResource ResourceKey=MarkerBrush}" />
        <Setter Property="Opacity" Value="0.75" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="controls:TreeListViewDragCue">
                    <Grid>
                        <mat:Shadow Background="{telerik1:FluentResource ResourceKey=PrimaryBrush}" ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" />
                        <StackPanel>
                            <Grid
                                MinHeight="28"
                                HorizontalAlignment="Left"
                                Visibility="{TemplateBinding DragPreviewVisibility}">
                                <Grid.Resources>
                                    <Style TargetType="TextBlock">
                                        <Setter Property="Foreground" Value="{telerik1:FluentResource ResourceKey=MarkerBrush}" />
                                    </Style>
                                </Grid.Resources>
                                <Border
                                    Background="{telerik1:FluentResource ResourceKey=PrimaryBrush}"
                                    BorderBrush="{telerik1:FluentResource ResourceKey=PrimaryBrush}"
                                    BorderThickness="0">
                                    <ItemsPresenter Margin="{TemplateBinding Padding}" />
                                </Border>
                            </Grid>
                            <Border
                                MinHeight="36"
                                Background="{telerik1:FluentResource ResourceKey=PrimaryBrush}"
                                BorderBrush="{telerik1:FluentResource ResourceKey=BasicBrush}"
                                BorderThickness="0"
                                Opacity="0.9"
