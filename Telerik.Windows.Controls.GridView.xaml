    <ControlTemplate x:Key="GridViewHeaderCellTemplate" TargetType="grid:GridViewHeaderCell">
        <Grid x:Name="PART_OuterGrid">
            <Grid.RowDefinitions>
                <RowDefinition Height="2*" />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <Grid x:Name="PART_HeaderCellGrid">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="4" />
                </Grid.ColumnDefinitions>
                <Border
                    x:Name="GridViewHeaderCell"
                    Grid.ColumnSpan="5"
                    MinHeight="{TemplateBinding MinHeight}"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}">
                    <Grid>
                        <Border
                            x:Name="GridViewHeaderCell_Over"
                            Background="{TemplateBinding mat:MaterialAssist.MouseOverBrush}"
                            Visibility="Collapsed" />
                        <Border
                            x:Name="GridViewHeaderCell_Selected"
                            Background="{TemplateBinding mat:MaterialAssist.CheckedBrush}"
                            Visibility="Collapsed" />
                    </Grid>
                </Border>
                <ContentControl
                    x:Name="ContentPresenter"
                    Grid.Column="0"
                    Margin="{TemplateBinding Padding}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    Content="{TemplateBinding Content}"
                    ContentTemplate="{TemplateBinding ContentTemplate}"
                    Foreground="{TemplateBinding Foreground}"
                    IsTabStop="{TemplateBinding IsTabStop}" />
                <telerik:RadGlyph
                    x:Name="PART_SortIndicator"
                    Grid.ColumnSpan="5"
                    Margin="0,-2,0,0"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Top"
                    FontSize="16"
                    Foreground="{telerik1:FluentResource ResourceKey=IconBrush}"
                    Glyph="{StaticResource GlyphArrowChevronUp}"
                    Visibility="Collapsed" />
                <ContentControl
                    x:Name="ContentControl"
                    Grid.Column="2"
                    Margin="0,0,2,4"
                    VerticalAlignment="Center"
                    Content="{Binding SortingIndex, Converter={StaticResource SortingIndexConverter}, RelativeSource={RelativeSource TemplatedParent}}"
                    FontSize="{telerik1:FluentResource ResourceKey=FontSizeS}"
                    Foreground="{telerik1:FluentResource ResourceKey=MarkerBrush}"
                    IsTabStop="{TemplateBinding IsTabStop}"
                    Visibility="{Binding SortingIndex, Converter={StaticResource SortingIndexToVisibilityConverter}, RelativeSource={RelativeSource TemplatedParent}}" />
                <grid:FilteringDropDown
                    Name="PART_DistinctFilterControl"
                    Grid.Column="3"
                    VerticalAlignment="Center"
                    Visibility="{TemplateBinding FilteringUIVisibility}" />
                <Thumb
                    x:Name="PART_LeftHeaderGripper"
                    Grid.ColumnSpan="5"
                    HorizontalAlignment="Left"
                    IsTabStop="{TemplateBinding IsTabStop}"
                    Style="{StaticResource ColumnHeaderGripperStyle}" />
                <Thumb
                    x:Name="PART_RightHeaderGripper"
                    Grid.ColumnSpan="5"
                    HorizontalAlignment="Right"
                    IsTabStop="{TemplateBinding IsTabStop}"
                    Style="{StaticResource ColumnHeaderGripperStyle}" />
            </Grid>
            <grid:FieldFilterControl
                x:Name="PART_FieldFilterControl"
                Grid.Row="1"
                MinHeight="31"
                Visibility="{TemplateBinding FieldFilterControlVisibility}" />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="SortingState" Value="None">
                <Setter TargetName="PART_SortIndicator" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="PART_DistinctFilterControl" Property="Foreground" Value="{telerik1:FluentResource ResourceKey=IconBrush}" />
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="GridViewHeaderCell_Over" Property="Visibility" Value="Visible" />
            </Trigger>
            <Trigger Property="SortingState" Value="Ascending">
                <Setter TargetName="GridViewHeaderCell_Selected" Property="Visibility" Value="Visible" />
                <Setter TargetName="PART_SortIndicator" Property="Visibility" Value="Visible" />
                <Setter TargetName="PART_SortIndicator" Property="Foreground" Value="{telerik1:FluentResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="ContentControl" Property="Foreground" Value="{telerik1:FluentResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="ContentPresenter" Property="TextBlock.Foreground" Value="{telerik1:FluentResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="PART_DistinctFilterControl" Property="Foreground" Value="{telerik1:FluentResource ResourceKey=MarkerInvertedBrush}" />
            </Trigger>
            <Trigger Property="SortingState" Value="Descending">
                <Setter TargetName="PART_SortIndicator" Property="Visibility" Value="Visible" />
                <Setter TargetName="PART_SortIndicator" Property="Glyph" Value="{StaticResource GlyphArrowChevronDown}" />
                <Setter TargetName="PART_SortIndicator" Property="Foreground" Value="{telerik1:FluentResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="PART_DistinctFilterControl" Property="Foreground" Value="{telerik1:FluentResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="ContentControl" Property="Foreground" Value="{telerik1:FluentResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="GridViewHeaderCell_Selected" Property="Visibility" Value="Visible" />
                <Setter TargetName="ContentPresenter" Property="TextBlock.Foreground" Value="{telerik1:FluentResource ResourceKey=MarkerInvertedBrush}" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
