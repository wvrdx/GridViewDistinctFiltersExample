                            <CheckBox
                                x:Name="PART_SelectAllCheckBox"
                                Margin="0,0,0,5"
                                telerik:LocalizationManager.ResourceKey="GridViewFilterSelectAll"
                                IsChecked="{Binding SelectAll, Mode=TwoWay}" />
