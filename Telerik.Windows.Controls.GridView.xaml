                                x:Name="PART_ClearFilterButton"
                                Grid.Column="1"
                                MinWidth="100"
                                Margin="5,5,0,0"
                                telerik:LocalizationManager.ResourceKey="GridViewClearFilter" />
                        </Grid>
                    </StackPanel>
                    <telerik:RadButton
                        x:Name="PART_FilterCloseButton"
                        Grid.Column="1"
                        Width="16"
                        Height="16"
                        MinHeight="0"
                        Margin="{TemplateBinding Padding}"
                        Padding="0"
