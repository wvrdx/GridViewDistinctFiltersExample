                        BorderThickness="0">
                        <telerik:RadGlyph
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            FontSize="16"
                            Foreground="{telerik1:FluentResource ResourceKey=IconBrush}"
                            Glyph="{StaticResource GlyphClose}" />
                    </telerik:RadButton>
                </Grid>
            </Border>
        </Grid>
