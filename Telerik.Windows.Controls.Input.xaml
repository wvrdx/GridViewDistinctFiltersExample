    <ControlTemplate x:Key="RadCalculatorTemplate" TargetType="controls:RadCalculator">
        <Grid Background="Transparent" SnapsToDevicePixels="True">
            <Border
                Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}"
                CornerRadius="{telerik1:FluentResource ResourceKey=CornerRadius}"
                SnapsToDevicePixels="True">
                <Grid x:Name="Root" Margin="{TemplateBinding Padding}">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Border
                        MinHeight="90"
                        Margin="0"
                        Padding="0"
                        Background="{x:Null}"
                        BorderBrush="{x:Null}"
                        SnapsToDevicePixels="True">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <TextBox
                                x:Name="DisplayTextBox"
                                Grid.Row="1"
                                Grid.ColumnSpan="2"
                                Foreground="{TemplateBinding Foreground}"
                                Style="{StaticResource CalculatorDisplayValue_Style}"
                                Text="{Binding DisplayedValue, RelativeSource={RelativeSource TemplatedParent}}" />
                            <Canvas>
                                <TextBlock
                                    x:Name="MemoryIndicatorTextBox"
                                    Margin="8,8,0,0"
                                    VerticalAlignment="Top"
                                    FontSize="{telerik1:FluentResource ResourceKey=FontSize}"
                                    Text="M"
                                    Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsMemoryUpdated, Converter={StaticResource BooleanToVisibilityConverter}}" />
                            </Canvas>
                            <TextBox
                                x:Name="HistoryTextBox"
                                Grid.Column="1"
                                Foreground="{TemplateBinding Foreground}"
                                Style="{StaticResource CalculatorHistoryDisplay_Style}"
                                Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=History, Mode=OneWay}" />
                        </Grid>
                    </Border>
                    <Grid x:Name="btnContainer" Grid.Row="1">
                        <Grid.RowDefinitions>
                            <RowDefinition x:Name="row" />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition />
                            <ColumnDefinition />
                            <ColumnDefinition />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <telerik:RadButton
                            x:Name="Mc_Button"
                            Grid.Column="0"
                            Command="{x:Static controls:RadCalculatorCommands.MemoryClear}"
                            Content="MC"
                            Style="{StaticResource CalculatorMemoryButtonStyle}" />
                        <telerik:RadButton
                            x:Name="Mr_Button"
                            Grid.Column="1"
                            Command="{x:Static controls:RadCalculatorCommands.MemoryRecall}"
                            Content="MR"
                            Style="{StaticResource CalculatorMemoryButtonStyle}" />
                        <telerik:RadButton
                            x:Name="Ms_Button"
                            Grid.Column="2"
                            Command="{x:Static controls:RadCalculatorCommands.MemoryStore}"
                            Content="MS"
                            Style="{StaticResource CalculatorMemoryButtonStyle}" />
                        <telerik:RadButton
                            x:Name="Mplus_Button"
                            Grid.Column="3"
                            Command="{x:Static controls:RadCalculatorCommands.MemoryPlus}"
                            Content="M+"
                            Style="{StaticResource CalculatorMemoryButtonStyle}" />
                        <telerik:RadButton
                            x:Name="Mminus_Button"
                            Grid.Column="4"
                            Command="{x:Static controls:RadCalculatorCommands.MemoryMinus}"
                            Content="M-"
                            Style="{StaticResource CalculatorMemoryButtonStyle}" />
                        <telerik:RadButton
                            x:Name="Delete_Button"
                            Grid.Row="1"
                            Grid.Column="0"
                            Command="{x:Static controls:RadCalculatorCommands.Delete}"
                            Content="←"
                            Style="{StaticResource CalculatorAlternateButtonStyle}" />
                        <telerik:RadButton
                            x:Name="ClearInput_Button"
                            Grid.Row="1"
                            Grid.Column="1"
                            Command="{x:Static controls:RadCalculatorCommands.ClearInput}"
                            Content="CE"
                            Style="{StaticResource CalculatorAlternateButtonStyle}" />
                        <telerik:RadButton
                            x:Name="ClearEverything_Button"
                            Grid.Row="1"
                            Grid.Column="2"
