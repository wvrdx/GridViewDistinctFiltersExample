                            CommandParameter="4"
                            Content="4" />
                        <telerik:RadButton
                            x:Name="input5_Button"
                            Grid.Row="3"
                            Grid.Column="1"
                            Command="{x:Static controls:RadCalculatorCommands.UpdateInput}"
                            CommandParameter="5"
                            Content="5" />
                        <telerik:RadButton
                            x:Name="input6_Button"
                            Grid.Row="3"
                            Grid.Column="2"
                            Command="{x:Static controls:RadCalculatorCommands.UpdateInput}"
                            CommandParameter="6"
                            Content="6" />
                        <telerik:RadButton
                            x:Name="Multiply_Button"
                            Grid.Row="3"
                            Grid.Column="3"
                            Command="{x:Static controls:RadCalculatorCommands.AddPendingBinaryOperation}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=Operations.Multiply}"
                            Content="*" />
                        <telerik:RadButton
                            x:Name="Reciprocal_Button"
                            Grid.Row="3"
                            Grid.Column="4"
                            Command="{x:Static controls:RadCalculatorCommands.ExecuteUnaryOperation}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=Operations.Reciprocal}"
                            Content="1/x"
                            Style="{StaticResource CalculatorAlternateButtonStyle}" />
                        <telerik:RadButton
                            x:Name="input1_Button"
                            Grid.Row="4"
                            Grid.Column="0"
                            Command="{x:Static controls:RadCalculatorCommands.UpdateInput}"
                            CommandParameter="1"
                            Content="1" />
                        <telerik:RadButton
                            x:Name="input2_Button"
                            Grid.Row="4"
                            Grid.Column="1"
                            Command="{x:Static controls:RadCalculatorCommands.UpdateInput}"
                            CommandParameter="2"
                            Content="2" />
                        <telerik:RadButton
                            x:Name="input3_Button"
                            Grid.Row="4"
                            Grid.Column="2"
                            Command="{x:Static controls:RadCalculatorCommands.UpdateInput}"
                            CommandParameter="3"
                            Content="3" />
                        <telerik:RadButton
                            x:Name="Substract_Button"
                            Grid.Row="4"
                            Grid.Column="3"
                            Command="{x:Static controls:RadCalculatorCommands.AddPendingBinaryOperation}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=Operations.Substract}"
                            Content="-" />
                        <telerik:RadButton
                            x:Name="input0_Button"
                            Grid.Row="5"
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            Command="{x:Static controls:RadCalculatorCommands.UpdateInput}"
                            CommandParameter="0"
                            Content="0" />
                        <telerik:RadButton
                            x:Name="FloatingPoint_Button"
                            Grid.Row="5"
                            Grid.Column="2"
                            Command="{x:Static controls:RadCalculatorCommands.UpdateInput}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=DecimalSeparator}"
                            Content="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=DecimalSeparator}" />
                        <telerik:RadButton
                            x:Name="Add_Button"
                            Grid.Row="5"
                            Grid.Column="3"
                            Command="{x:Static controls:RadCalculatorCommands.AddPendingBinaryOperation}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=Operations.Add}"
                            Content="+" />
                        <telerik:RadButton
                            x:Name="Equals_Button"
                            Grid.Row="4"
                            Grid.RowSpan="2"
                            Grid.Column="4"
                            Command="{x:Static controls:RadCalculatorCommands.ExecuteBinaryOperation}"
                            Content="="
                            Style="{StaticResource CalculatorAlternateButtonStyle}" />
                    </Grid>
                </Grid>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="MemoryButtonsVisibility" Value="Collapsed">
                <Setter TargetName="Mc_Button" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="Mr_Button" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="Ms_Button" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="Mplus_Button" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="Mminus_Button" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="row" Property="Height" Value="0" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="MemoryIndicatorTextBox" Property="Opacity" Value="{telerik1:FluentResource ResourceKey=DisabledOpacity}" />
                <Setter TargetName="HistoryTextBox" Property="Opacity" Value="{telerik1:FluentResource ResourceKey=DisabledOpacity}" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
