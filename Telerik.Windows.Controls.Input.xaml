                                Padding="{TemplateBinding Padding}"
                                VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                                AcceptsReturn="{TemplateBinding AcceptsReturn}"
                                AutomationProperties.Name="{TemplateBinding AutomationProperties.Name}"
                                Background="{TemplateBinding Background}"
                                CaretBrush="{telerik1:FluentResource ResourceKey=MarkerMouseOverBrush}"
                                Cursor="{TemplateBinding Cursor}"
                                FontFamily="{TemplateBinding FontFamily}"
                                FontSize="{TemplateBinding FontSize}"
                                FontWeight="{TemplateBinding FontWeight}"
                                Foreground="{TemplateBinding Foreground}"
                                IsEnabled="{TemplateBinding IsEnabled}"
                                IsReadOnly="{TemplateBinding IsReadOnly}"
                                Opacity="0"
                                Style="{TemplateBinding TextBoxStyle}"
                                TabIndex="{TemplateBinding TabIndex}"
                                TextAlignment="{Binding HorizontalContentAlignment, Converter={StaticResource conv}, RelativeSource={RelativeSource Mode=TemplatedParent}}" />
                            <Border
                                x:Name="DisplayTextContent"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="0"
                                IsHitTestVisible="False">
                                <telerikMaskedInput:PreviewInputTextBox
                                    x:Name="DisplayText"
                                    Height="{TemplateBinding Height}"
                                    Padding="{TemplateBinding Padding}"
                                    HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
                                    VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                                    AutomationProperties.Name="{TemplateBinding AutomationProperties.Name}"
                                    Cursor="{TemplateBinding Cursor}"
                                    FontFamily="{TemplateBinding FontFamily}"
                                    FontSize="{TemplateBinding FontSize}"
                                    FontWeight="{TemplateBinding FontWeight}"
                                    Foreground="{TemplateBinding Foreground}"
                                    IsEnabled="{TemplateBinding IsEnabled}"
                                    IsReadOnly="{TemplateBinding IsReadOnly}"
                                    IsTabStop="False"
                                    Opacity="{telerik1:FluentResource ResourceKey=InputOpacity}"
                                    Style="{TemplateBinding TextBoxStyle}"
                                    Text="{Binding Path=Text, RelativeSource={RelativeSource Mode=TemplatedParent}, Mode=OneWay}"
                                    TextAlignment="{Binding HorizontalContentAlignment, Converter={StaticResource conv}, RelativeSource={RelativeSource Mode=TemplatedParent}}" />
                            </Border>
                            <Border
                                x:Name="EmptyContent"
                                IsHitTestVisible="False"
                                Visibility="Collapsed">
                                <ContentControl
                                    x:Name="Content"
                                    Margin="{TemplateBinding Padding}"
                                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                    Content="{TemplateBinding EmptyContent}"
                                    ContentTemplate="{TemplateBinding EmptyContentTemplate}"
                                    FontStyle="Italic"
                                    Foreground="{TemplateBinding Foreground}"
                                    IsTabStop="False"
                                    Opacity="{telerik1:FluentResource ResourceKey=InputOpacity}" />
                            </Border>
                            <telerik1:RadButton
                                x:Name="ClearButton"
                                Grid.Column="1"
                                Command="{TemplateBinding ClearCommand}"
                                Opacity="0"
                                Style="{TemplateBinding ClearButtonStyle}"
                                Visibility="{Binding Path=IsClearButtonVisible, RelativeSource={RelativeSource Mode=TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}" />
                        </Grid>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsReadOnly" Value="True">
                            <Setter TargetName="Content" Property="Opacity" Value="{telerik1:FluentResource ResourceKey=ReadOnlyOpacity}" />
                            <Setter TargetName="DisplayText" Property="Opacity" Value="{telerik1:FluentResource ResourceKey=ReadOnlyOpacity}" />
                            <Setter TargetName="ReadOnlyVisual" Property="Visibility" Value="Visible" />
                        </Trigger>
                        <Trigger Property="IsKeyboardFocusWithin" Value="True">
                            <Setter Property="Foreground" Value="{telerik1:FluentResource ResourceKey=MarkerMouseOverBrush}" />
                            <Setter TargetName="BorderVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.MouseOverBrush), Mode=OneWay}" />
                            <Setter TargetName="DisplayTextContent" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="EditorSite" Property="Opacity" Value="1" />
                            <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible" />
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Foreground" Value="{telerik1:FluentResource ResourceKey=MarkerMouseOverBrush}" />
                            <Setter TargetName="BorderVisual" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.MouseOverBrush), Mode=OneWay}" />
                            <Setter TargetName="Content" Property="Opacity" Value="1" />
                            <Setter TargetName="DisplayText" Property="Opacity" Value="1" />
                            <Setter TargetName="ClearButton" Property="Opacity" Value="1" />
                        </Trigger>

                        <!--  Content State - Empty  -->
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsKeyboardFocusWithin" Value="False" />
                                <Condition Property="IsEmpty" Value="True" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="EmptyContent" Property="Visibility" Value="Visible" />
                            <Setter TargetName="DisplayTextContent" Property="Visibility" Value="Collapsed" />
                        </MultiTrigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="Content" Property="Opacity" Value="{telerik1:FluentResource ResourceKey=DisabledOpacity}" />
                            <Setter TargetName="DisplayText" Property="Opacity" Value="{telerik1:FluentResource ResourceKey=DisabledOpacity}" />
                        </Trigger>

                        <!--  Invalid  -->
                        <Trigger Property="Validation.HasError" Value="True">
                            <Setter TargetName="FocusVisual" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
