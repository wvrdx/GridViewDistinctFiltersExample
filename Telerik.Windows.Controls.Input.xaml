                                </controls:RadColorPaletteView.ItemsPanel>
                            </controls:RadColorPaletteView>
                            <Border
                                x:Name="AdditionalContentBorder"
                                Grid.Row="8"
                                Visibility="Collapsed">
                                <ContentPresenter Content="{TemplateBinding AdditionalContent}" ContentTemplate="{TemplateBinding AdditionalContentTemplate}" />
