                                    VerticalAlignment="Center"
                                    FontSize="{TemplateBinding FontSize}"
                                    FontWeight="{TemplateBinding FontWeight}"
                                    Foreground="{TemplateBinding Foreground}"
                                    Text="{TemplateBinding StandardPaletteHeaderText}" />
                            </Border>
                            <controls:RadColorPaletteView
                                x:Name="StandartPalette"
                                Grid.Row="5"
                                ColorPropertyPath="{TemplateBinding ColorPropertyPath}"
                                ItemTemplate="{TemplateBinding PaletteItemsTemplate}">
                                <controls:RadColorPaletteView.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <primitives:RadUniformGrid Background="Transparent" Columns="10" />
                                    </ItemsPanelTemplate>
                                </controls:RadColorPaletteView.ItemsPanel>
                            </controls:RadColorPaletteView>
                            <Border
                                x:Name="RecentColorsPaletteBorder"
                                Grid.Row="6"
                                MinHeight="28"
                                Background="{telerik1:FluentResource ResourceKey=AlternativeBrush}"
                                Visibility="Collapsed">
                                <TextBlock
                                    x:Name="RecentColorsPaletteHeader"
                                    Margin="10,0"
                                    VerticalAlignment="Center"
                                    FontSize="{TemplateBinding FontSize}"
                                    FontWeight="{TemplateBinding FontWeight}"
                                    Foreground="{TemplateBinding Foreground}"
                                    Text="{TemplateBinding RecentColorsHeaderText}" />
                            </Border>
                            <controls:RadColorPaletteView
                                x:Name="RecentColorsPalette"
                                Grid.Row="7"
                                Background="{TemplateBinding Background}"
                                BorderThickness="0"
                                ColorPropertyPath="{TemplateBinding ColorPropertyPath}"
                                ItemTemplate="{TemplateBinding PaletteItemsTemplate}"
                                Visibility="Collapsed">
                                <controls:RadColorPaletteView.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <primitives:RadUniformGrid
                                            Background="Transparent"
                                            Columns="10"
                                            Rows="1" />
