                                MinWidth="1"
                                MinHeight="1"
                                HorizontalAlignment="Right"
                                VerticalAlignment="Stretch"
                                Fill="{TemplateBinding BorderBrush}"
                                Stroke="{x:Null}"
                                StrokeThickness="0" />
                            <telerik1:RadToggleButton
