                            </Border>
                            <controls:RadColorPaletteView
                                x:Name="HeaderPalette"
                                Grid.Row="2"
                                Padding="8,8,8,3"
                                ColorPropertyPath="{TemplateBinding ColorPropertyPath}"
                                ItemTemplate="{TemplateBinding PaletteItemsTemplate}">
                                <controls:RadColorPaletteView.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <primitives:RadUniformGrid Background="Transparent" Columns="10" />
                                    </ItemsPanelTemplate>
                                </controls:RadColorPaletteView.ItemsPanel>
                            </controls:RadColorPaletteView>
                            <controls:RadColorPaletteView
                                x:Name="MainPalette"
                                Grid.Row="3"
                                ColorPropertyPath="{TemplateBinding ColorPropertyPath}"
                                ItemContainerStyle="{TemplateBinding MainPaletteItemsStyle}"
                                ItemTemplate="{TemplateBinding PaletteItemsTemplate}"
                                PaletteColumnsCount="{TemplateBinding MainPaletteColumnsCount}" />
                            <Border
                                x:Name="StandardPaletteBorder"
                                Grid.Row="4"
                                MinHeight="28"
                                Background="{telerik1:FluentResource ResourceKey=AlternativeBrush}">
                                <TextBlock
                                    x:Name="standardPaletteHeader"
