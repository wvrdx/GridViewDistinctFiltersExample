    <colorPickerConverters:EnumConverter x:Key="EnumConverter" />
    <colorPickerConverters:ColorToHSColorBrushConverter x:Key="ColorToHSColorBrushConverter" />
    <colorMode:ColorModeCollection x:Key="ColorModeCollection" />
