    <Style x:Key="RadSplitContainerStyle" TargetType="local:RadSplitContainer">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:RadSplitContainer">
                    <Grid>
                        <Border
                            x:Name="BorderVisual"
                            Padding="{TemplateBinding Padding}"
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                            <ItemsPresenter />
                        </Border>
                        <dock:RadGridResizer
                            x:Name="DockResizer"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Stretch"
                            Placement="Left"
                            ShowsPreview="True"
                            Visibility="Collapsed" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="SplitterPosition">
                            <Trigger.Value>
                                <Dock>Left</Dock>
                            </Trigger.Value>
                            <Setter TargetName="DockResizer" Property="Visibility" Value="Visible" />
                            <Setter TargetName="DockResizer" Property="Width" Value="10" />
                            <Setter TargetName="BorderVisual" Property="Margin" Value="10,0,0,0" />
                            <Setter TargetName="DockResizer" Property="Cursor" Value="SizeWE" />
                        </Trigger>
                        <Trigger Property="SplitterPosition">
                            <Trigger.Value>
                                <Dock>Top</Dock>
                            </Trigger.Value>
                            <Setter TargetName="DockResizer" Property="Visibility" Value="Visible" />
                            <Setter TargetName="DockResizer" Property="VerticalAlignment" Value="Top" />
                            <Setter TargetName="DockResizer" Property="HorizontalAlignment" Value="Stretch" />
                            <Setter TargetName="DockResizer" Property="Cursor" Value="SizeNS" />
                            <Setter TargetName="DockResizer" Property="Placement">
                                <Setter.Value>
                                    <Dock>Top</Dock>
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="DockResizer" Property="Height" Value="10" />
                            <Setter TargetName="BorderVisual" Property="Margin" Value="0,10,0,0" />
                        </Trigger>
                        <Trigger Property="SplitterPosition">
                            <Trigger.Value>
                                <Dock>Right</Dock>
                            </Trigger.Value>
                            <Setter TargetName="DockResizer" Property="Visibility" Value="Visible" />
                            <Setter TargetName="DockResizer" Property="Width" Value="10" />
                            <Setter TargetName="BorderVisual" Property="Margin" Value="0,0,10,0" />
                            <Setter TargetName="DockResizer" Property="HorizontalAlignment" Value="Right" />
                            <Setter TargetName="DockResizer" Property="Cursor" Value="SizeWE" />
                            <Setter TargetName="DockResizer" Property="Placement">
                                <Setter.Value>
                                    <Dock>Right</Dock>
                                </Setter.Value>
                            </Setter>
                        </Trigger>
                        <Trigger Property="SplitterPosition">
                            <Trigger.Value>
                                <Dock>Bottom</Dock>
                            </Trigger.Value>
                            <Setter TargetName="DockResizer" Property="Cursor" Value="SizeNS" />
                            <Setter TargetName="DockResizer" Property="Visibility" Value="Visible" />
                            <Setter TargetName="DockResizer" Property="VerticalAlignment" Value="Bottom" />
                            <Setter TargetName="DockResizer" Property="HorizontalAlignment" Value="Stretch" />
                            <Setter TargetName="DockResizer" Property="Height" Value="10" />
                            <Setter TargetName="DockResizer" Property="Placement">
                                <Setter.Value>
                                    <Dock>Bottom</Dock>
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="BorderVisual" Property="Margin" Value="0,0,0,10" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="MinWidth" Value="15" />
        <Setter Property="MinHeight" Value="15" />
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <local:ProportionalStackPanel />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="PaneHeaderControlTemplate" TargetType="dock:PaneHeader">
        <Border
            x:Name="LayoutRoot"
            Background="{TemplateBinding Background}"
            BorderBrush="{TemplateBinding BorderBrush}"
            BorderThickness="{TemplateBinding BorderThickness}">
            <Grid x:Name="Content">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="0" />
                </Grid.ColumnDefinitions>
                <ContentControl
                    x:Name="TitleElement"
                    Margin="{TemplateBinding Padding}"
                    VerticalAlignment="{TemplateBinding VerticalAlignment}"
                    HorizontalContentAlignment="Stretch"
                    Content="{Binding SelectedPane.Title, RelativeSource={RelativeSource TemplatedParent}}"
                    ContentTemplate="{Binding SelectedPane.TitleTemplate, RelativeSource={RelativeSource TemplatedParent}}"
                    FontFamily="{TemplateBinding FontFamily}"
                    FontSize="{TemplateBinding FontSize}"
                    Foreground="{TemplateBinding Foreground}"
                    IsTabStop="False" />
                <telerik:RadDropDownButton
                    x:Name="HeaderDropDownMenu"
                    Grid.Column="1"
                    Margin="0"
                    Command="local:RadDockingCommands.ContextMenuOpen"
                    CommandParameter="{TemplateBinding SelectedPane}"
                    IsTabStop="{TemplateBinding IsTabStop}"
                    Style="{StaticResource DropDownButtonDropDownStyle}" />
                <Grid
                    Grid.Column="2"
                    MinWidth="28"
                    Margin="0"
                    Visibility="{Binding SelectedPane.PinButtonVisibility, RelativeSource={RelativeSource TemplatedParent}}">
                    <telerik:RadButton
                        x:Name="HeaderPinButton"
                        Command="local:RadDockingCommands.Pin"
                        CommandParameter="{TemplateBinding SelectedPane}"
                        IsTabStop="{TemplateBinding IsTabStop}"
                        Style="{StaticResource RadButtonPinStyle}" />
                    <telerik:RadButton
                        x:Name="HeaderUnPinButton"
                        Command="local:RadDockingCommands.Pin"
                        CommandParameter="{TemplateBinding SelectedPane}"
                        IsTabStop="{TemplateBinding IsTabStop}"
                        Style="{StaticResource RadButtonUnpinStyle}"
                        Visibility="Collapsed" />
                </Grid>
                <telerik:RadButton
                    x:Name="HeaderCloseButton"
                    Grid.Column="3"
                    Margin="0,0,5,0"
                    VerticalAlignment="Center"
                    Command="local:RadDockingCommands.Close"
                    CommandParameter="{TemplateBinding SelectedPane}"
                    IsTabStop="{TemplateBinding IsTabStop}"
                    Style="{StaticResource RadButtonCloseStyle}" />
            </Grid>
        </Border>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding SelectedPane.IsPinned, RelativeSource={RelativeSource TemplatedParent}}" Value="False">
                <Setter TargetName="HeaderUnPinButton" Property="Visibility" Value="Visible" />
                <Setter TargetName="HeaderPinButton" Property="Visibility" Value="Collapsed" />
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=SelectedPane.(telerikNavigation:RadContextMenu.ContextMenu).HasItems, RelativeSource={RelativeSource TemplatedParent}}" Value="False">
                <Setter TargetName="HeaderDropDownMenu" Property="Visibility" Value="Collapsed" />
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=SelectedPane.ContextMenuTemplate, RelativeSource={RelativeSource TemplatedParent}}" Value="{x:Null}">
                <Setter TargetName="HeaderDropDownMenu" Property="Visibility" Value="Collapsed" />
            </DataTrigger>
            <Trigger Property="IsHighlighted" Value="True">
                <Setter TargetName="LayoutRoot" Property="Background" Value="{telerik:FluentResource ResourceKey=AccentBrush}" />
                <Setter TargetName="HeaderPinButton" Property="Foreground" Value="{telerik:FluentResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="HeaderUnPinButton" Property="Foreground" Value="{telerik:FluentResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="HeaderCloseButton" Property="Foreground" Value="{telerik:FluentResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="HeaderDropDownMenu" Property="Foreground" Value="{telerik:FluentResource ResourceKey=MarkerInvertedBrush}" />
                <Setter TargetName="TitleElement" Property="Foreground" Value="{telerik:FluentResource ResourceKey=MarkerInvertedBrush}" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="TitleElement" Property="Opacity" Value="{telerik:FluentResource ResourceKey=DisabledOpacity}" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="PaneHeaderStyle" TargetType="dock:PaneHeader">
